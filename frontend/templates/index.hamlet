<div .row>
  <div .panel .panel-default .center>
    <div .panel-body>
      <div .row .bottom-padded>
        <div .col-md-2 .profile-col-fix .col-sm-12>
            <img .thumbnail .profile-image src="@{ProfilePhotoR (T.unpack (memberUsername usr))}">
        <div .col-md-10 .col-sm-12>
          <h2>
            <a href=@{MemberR $ T.unpack (memberUsername usr)}">#{memberCommonname usr}
          <span .label .label-success>Active
          $if (and (map eboard
          <span .label .label-success>Eboard
      <div #profile-nav .row .bottom-padded-30>
        <ul .nav .nav-justified .nav-tabs>
          <li .active>
            <a href=""> Evaluations
          <li>
            <a href=""> History
          <li>
            <a href=""> Projects
          <li>
            <a href=""> Seminars
          <li>
            <a href=""> Attendance


      $#$if ((==) access AdminAccess)
      $#  <div .panel .panel-default>
      $#    <div .panel-heading>
      $#      <div .panel-title>
      $#        <strong> Admin Panel
      $#    <div .panel-body>
      $#      Admin panel body!
      <div .row>
          <div .panel>
              <div .panel-header>
                  <div .col-md-2>
                      <h3> 2015-2016 Evaluation
              <div .panel-body>
                  <!--div #grid data-columns-->
                    <div .col-md-3>
                        <div .panel .panel-default>
                          <div .panel-heading>
                            <div .panel-title>
                              <strong> Summary
                          <div .panel-body .table-fill>
                            <table .table .table-striped .table-responsive .no-bottom-margin>
                              <tbody>
                                <tr>
                                  <th>Points
                                  <td>41
                                <tr>
                                  <th>Committee Meetings
                                  <td>25
                                <tr>
                                  <th>Major Projects
                                  <td>0
                                <tr>
                                  <th>House Meetings Missed
                                  <td>0
                                <tr>
                                  <th>Social Events
                                  <td>7
                                <tr>
                                  <th>Technical Seminars
                                  <td>13
                          <div .panel-inner-padding>
                            <strong>Notes
                            <br>
                            None
                <div .col-md-7>
                    <div .panel .panel-default>
                      <div .panel-heading>
                        <div .panel-title>
                          <strong>Projects
                      <div .panel-body>
                        <div .text-center .bottom-padded>
                            <h4 .text-muted>
                              <strong>No Projects
                            <a .btn .btn-primary href=@{CreateProjectR}>
                              Create One
                    <div .panel .panel-default>
                      <div .panel-heading>
                        <div .panel-title>
                          <strong>Attendance
                      <div .panel-body .table-fill>
                        <table .table .table-striped .table-responsive .no-bottom-margin>
                          <tbody>
                            <tr>
                              <th>Name
                              <th>Type
                              <th>Date
                            $forall (name, meetingType, date) <- attendance
                              <tr>
                                <td>#{B.unpack name}
                                <td>#{B.unpack meetingType}
                                <td>#{B.unpack date}
